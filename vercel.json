{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "bun install",
  "fluid": true,
  "functions": {
    "api/vercel.js": {
      "includeFiles": "assets/**/*"
    }
  },
  "rewrites": [
    { "source": "/:path*", "destination": "/api/vercel" },
    { "source": "/", "destination": "/api/vercel" }
  ]
}
